{
  "name": "@gapi/core",
  "version": "0.7.197",
  "repository": {
    "type": "git",
    "url": "http://gitlab.youvolio.com/open-source/gapi.git"
  },
  "author": "Kristian Tachev <kristiqn.tachev@gmail.com> (Stradivario)",
  "scripts": {
    "postpublish": "git push ssh://git@gitlab.youvolio.com:522/gapi/gapi.git -f && git push ssh://git@gitlab.youvolio.com:522/gapi/gapi.git --tags",
    "lint": "tslint -c tslint.json 'development/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "ammend": "git add . && git commit --amend && git push -f && git push -f git@github.com:Stradivario/gapi-core.git"
  },
  "keywords": [
    "graphql",
    "gapi",
    "node"
  ],
  "license": "MIT",
  "bugs": {
    "url": "http://gitlab.youvolio.com/open-source/gapi/issues"
  },
  "module": "gapi-module.js",
  "typings": "gapi-module.d.ts",
  "devDependencies": {
    "@types/graphql": "^0.12.4",
    "@types/hapi": "^17.0.11",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.7",
    "graphql-tester": "0.0.5",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "ts-jest": "^22.4.0",
    "tslint": "^5.8.0",
    "tslint-language-service": "^0.9.8"
  },
  "dependencies": {
    "@gapi/amqp": "^0.7.196",
    "apollo-codegen": "^0.18.3",
    "apollo-errors": "^1.5.1",
    "apollo-server-hapi": "^1.3.6",
    "apollo-upload-server": "^4.0.2",
    "banyan": "github:Stradivario/banyan",
    "boom": "^7.2.0",
    "fs-extra": "^5.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.14.1",
    "hapi": "^17.4.0",
    "jsonwebtoken": "^8.1.0",
    "pg": "^6.4.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "typescript": "^2.7.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/development/.*\\.spec.(ts|tsx|js)$",
    "verbose": true,
    "collectCoverage": true
  },
  "description": "![Build Status](http://gitlab.youvolio.com/gapi/gapi-starter/badges/master/build.svg)",
  "main": "index.js"
}
