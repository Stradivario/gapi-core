{
  "name": "@gapi/core",
  "version": "0.7.2",
  "repository": {
    "type": "git",
    "url": "http://gitlab.youvolio.com/open-source/gapi.git"
  },
  "author": "Kristian Tachev <kristiqn.tachev@gmail.com> (Stradivario)",
  "scripts": {
    "lint":"echo lint",
    "ammend": "git add . && git commit --amend && git push -f && git push -f git@github.com:Stradivario/gapi.git"
  },
  "keywords": [
    "graphql",
    "gapi",
    "node"
  ],
  "license": "MIT",
  "bugs": {
    "url": "http://gitlab.youvolio.com/open-source/gapi/issues"
  },
  "module": "gapi-module.js",
  "typings": "gapi-module.d.ts",
  "devDependencies": {
    "@types/graphql": "^0.12.4",
    "@types/hapi": "^16.1.9",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.7",
    "graphql-tester": "0.0.5",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "ts-jest": "^22.4.0",
    "tslint": "^5.8.0",
    "tslint-language-service": "^0.9.8"
  },
  "dependencies": {
    "@gapi/amqp": "0.0.1",
    "apollo-codegen": "^0.18.3",
    "apollo-errors": "^1.5.1",
    "apollo-server-hapi": "^1.3.2",
    "apollo-upload-server": "^4.0.2",
    "banyan": "github:Stradivario/banyan",
    "boom": "^7.2.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.14.1",
    "hapi": "^16.6.2",
    "jsonwebtoken": "^8.1.0",
    "pg": "^6.4.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "sequelize": "^4.35.2",
    "sequelize-typescript": "^0.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/graphql/.*\\.spec.(ts|tsx|js)$",
    "verbose": true,
    "collectCoverage": true
  },
  "description": "![Build Status](http://gitlab.youvolio.com/gapi/gapi-starter/badges/master/build.svg)",
  "main": "index.js"
}
